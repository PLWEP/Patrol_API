{
  "name": "patrol",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "migrate": "npx sequelize db:migrate",
    "undo-migrate": "npx sequelize db:migrate:undo",
