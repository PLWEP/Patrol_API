    "undo-seed": "npx sequelize db:seed:undo:all",
    "init": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "fastest-validator": "^1.15.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.0.0-rc.1",
    "nodemon": "^2.0.20",
    "sequelize": "^6.25.4",
    "sequelize-cli": "^6.5.2"
  }
}
